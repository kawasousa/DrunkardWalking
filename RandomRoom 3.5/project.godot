; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "GDSerComm",
"language": "NativeScript",
"path": "res://addons/GDSerCommDock/bin/GDSerComm.gdns"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://scripts/Player.gd"
} ]
_global_script_class_icons={
"GDSerComm": "",
"Player": ""
}

[application]

config/name="Random Room 3.5"
run/main_scene="res://scenes/Room.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://autoload/global.tscn"

[display]

window/size/fullscreen=true
window/stretch/mode="viewport"

[editor_plugins]

enabled=PoolStringArray( "res://addons/GDSerCommDock/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[layer_names]

2d_physics/layer_1="track"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
